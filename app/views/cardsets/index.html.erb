<div class="content container mt-6">
  <div class="columns">
    <div class="column">
      <h1>Sets</h1>
      <ul style="list-style-type: none;">
        <% @cardsets.each do |set|%>
          <li><%= link_to set.name, set %></li>
        <%end%>
      </ul>
    </div>
  </div>
</div>
<section class="section has-background-primary	">
  <nav class="level ">
    <div class="level-item">
      <div>
        <%= paginate @cardsets %>
      </div>
    </div>
  </nav>
</section>
