<!-- Locations block -->
  <h2 class="title">Locations</h2>
  <% for location in @locations %>
    <ul class="block">
      <li>
        <p><strong>Latitude:</strong> <%= location.latitude %></p>
      </li>
      <li>
        <p><strong>Longitude:</strong> <%= location.longitude %></p>
      </li>
    </ul>
  <% end %>
